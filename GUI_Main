                if (e.getStateChange() == ItemEvent.SELECTED) {

                    jLabel19.setIcon(new javax.swing.ImageIcon(getClass().getResource("/Kuvat/lock.png")));
                    System.out.println("jcb1 locked");

                } else {
                    jLabel19.setIcon(new javax.swing.ImageIcon(getClass().getResource("/Kuvat/unlock.png")));
